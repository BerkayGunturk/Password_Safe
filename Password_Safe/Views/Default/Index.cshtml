@using Core.Models
@model List<Account>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Hesaplar</h1>
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Başlık</th>
    </tr>

    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.UserName</td>
        </tr>
        
    }






</table>

